public Boolean isKeyAccount(Account a) {
    //code here
    if(Integer.valueOf(a.AnnualRevenue) == 800000 && a.Industry == 'Technology'){
        return true;
    }else if(Integer.valueOf(a.AnnualRevenue) == 600000 && a.Industry == 'Banking'){
        return true;
    }else if(Integer.valueOf(a.AnnualRevenue) == 2000000 && a.Industry == 'Retail'){
        return true;
    }else if(Integer.valueOf(a.AnnualRevenue) == 500000){
        return true;
    }
    return false;
}
